# Playback Server Configuration
# Copy this file to playback_config.ini and customize for your setup

[server]
# Flask API port
port = 7000

[location]
# Location coordinates for sunrise calculation
lat = 42.27
lon = -71.81

[playback]
# Offset in minutes from calculated sunrise time
# Positive = play after sunrise, Negative = play before sunrise
# Example: -15 means play 15 minutes before sunrise
offset_minutes = -15
